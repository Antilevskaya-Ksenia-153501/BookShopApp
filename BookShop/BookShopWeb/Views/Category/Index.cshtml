@model List<Category>

<h1>Category List</h1>
<table class="table table-bordered table-striped">
    <thead>
        <th>Name</th>
        <th>DisplayOrder</th>
    </thead>
    <tbody>
        @foreach (var el in Model)
        {
            <tr>
                <td>@el.Name</td>
                <td>@el.DisplayOrder</td>
            </tr>
        }
    </tbody>
</table>